<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4"
	xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<welcome-file-list>
		<welcome-file>myServ</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>MyServ</servlet-name>
		<servlet-class>com.example.servlet.MyServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MyServ</servlet-name>
		<url-pattern>/myServ</url-pattern>
	</servlet-mapping>
	<display-name>Servlet11</display-name>
	<context-param>
		<param-name>URL</param-name>
		<param-value>jdbc:oracle:thin:@//localhost:1521/XE</param-value>
	</context-param>
	<context-param>
		<param-name>USERID</param-name>
		<param-value>system</param-value>
	</context-param>
	<context-param>
		<param-name>PASSWORD</param-name>
		<param-value>system</param-value>
	</context-param>
	<servlet>
		<description>
		</description>
		<display-name>Launch</display-name>
		<servlet-name>Launch</servlet-name>
		<servlet-class>p11.Launch</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Launch</servlet-name>
		<url-pattern>/Launch</url-pattern>
	</servlet-mapping>
</web-app>
